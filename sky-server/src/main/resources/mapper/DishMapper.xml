<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sky.mapper.DishMapper">
    <insert id="insert">
        INSERT INTO dish
        (
            category_id,
            name,
            price,
            image,
            description,
            status
        )
        VALUES
        (
            #{categoryId},
            #{name},
            #{price},
            #{image},
            #{description},
            #{status}
        )
    </insert>
</mapper>
